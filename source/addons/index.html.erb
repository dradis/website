        <div class="hover-tile hover-tile-visible">
          <%= plugin[:title] %>
        </div>
        <div class="hover-tile hover-tile-hidden">
          <h4><%= plugin[:title] %></h4>
          <p><%= plugin[:description] %> <a href="/addons/<%= plugin[:page] %>.html">Read more<span>&#187;</span></a></p>
        </div>
      </div>
    </div>
  <% end %>
</div>

